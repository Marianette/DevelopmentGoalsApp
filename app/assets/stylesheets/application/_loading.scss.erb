.loading {
  position: absolute;
  top: inherit;
  left: 0;
  z-index: 3;
  background: url(<%= asset_path 'Rolling.gif' %>) 50% 40% no-repeat rgb(255,255,255);
}

.loading-left {
  width: inherit;
  height: 90%;
}

.loading-income {
  width: inherit;
  height: 400px;
}

.loading-compare {
  width: inherit;
  height: 90%;
  opacity: 0.75;
}

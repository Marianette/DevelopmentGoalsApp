<div class="row">
  <ul class="tabs">
    <li class="tab col s4"><a class="active" href="#basic">Basic Visualisation</a></li>
    <li class="tab col s4"><a href="#filters">Simple Filters</a></li>
    <li class="tab col s4"><a href="#interactive">Interactive</a></li>
  </ul>

  <!-- Basic bar chart -->
  <div id="basic" class="col s12" data-url="<%= home_basic_path %>">
    <div class="row">
      <div class="col s6">
        <h3> Bar Graph </h3>
        <svg id="basicgraph"></svg>
      </div>
      <div class="col s6">
        <h3>Scatterplot</h3>
        <svg id="circles"></svg>
      </div>
    </div>
  </div>

  <!-- Simple line graph that allows filter by key -->
  <div id="filters" class="col s12" data-url="<%= home_filters_path %>">
    <div id="linegraph"></div>
  </div>

  <!-- Interactive features -->
  <div id="interactive" class="col s12" data-url="<%= home_interactive_path %>">
    <svg id="interactivegraph"></svg>
  </div>
</div>

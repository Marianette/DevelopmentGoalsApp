<div class="content-container">
  <div class="row">
    <div class="col s12 income-vis-container">
      <div id="dot-plot-title">Estimated Gross National Income Per Capita</div>
      <%= tag('div', id: 'income-vis', data: {url: @data_url} ) %>
      <%= tag('/div') %>
    </div>
    <div class="col s12 income-filter-container">
      <div class="row">
        <div class="input-field col s1">
          <%= select_tag "select-income-year", options_for_select(@years.collect{ |y| [y, y]}, 2015) %>
          <label>Select Year</label>
        </div>
      </div>
    </div>
  </div>

  <script>
  createIncomeVis("#income-vis");
  </script>
